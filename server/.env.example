# Copy this file to .env and add your API keys

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# OpenWeather API Key
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Uber API Configuration
# Get these from https://developer.uber.com/
# Paste your keys here when you have them:
UBER_CLIENT_ID=your_uber_client_id_here
UBER_CLIENT_SECRET=your_uber_client_secret_here
UBER_SERVER_TOKEN=your_uber_server_token_here

# Set to 'false' for production, 'true' for sandbox/testing
UBER_SANDBOX_MODE=true

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# CORS Origins (comma-separated)
CORS_ORIGINS=*
